<!doctype html>
<html ng-app="contatooh">
    <head>
        <meta name="viewport" content="width=device-width">
        <meta charset="UTF-8">
        <title>Contatooh</title>
        <link rel="icon" href="images/favicon.ico">        
        <!-- build:css css/index.min.css -->        
        <link rel="stylesheet" href="vendor/bootstrap/dist/css/bootstrap.min.css">
        <link rel="stylesheet" href="vendor/bootstrap/dist/css/bootstrap-theme.min.css">
        <!-- endbuild -->
    </head>
    <body class="container" ng-controller="ContatosController">
        <div class="text-right">
            <p>Logado como: <span id="usuario-logado"><%=usuarioLogado%></span>
            <a href="/logout">Logout</a>
            </p>
        </div>
        <div ng-view></div>     
        <!-- build:js js/index.min.js -->
        <script src="vendor/angular/angular.min.js"></script>
        <script src="vendor/angular-route/angular-route.min.js"></script>
        <script src="vendor/angular-resource/angular-resource.min.js"></script>
        <script src="js/main.js"></script>
        <script src="js/controllers/ContatosController.js"></script>
        <script src="js/services/ContatoService.js"></script>
        <!-- endbuild -->
    </body>
</html>